                        <name type="place"><handShift rend="printed"/>Office of</name>
                        <name type="place">WAKEFIELD EARTH CLOSET CO.,</name>
                        <name type="place">34 Dey Street,</name>
                        <name type="place">New York,</name>
                    <date when="1876-09-23"><handShift rend="manuscript"/>Sept 23<hi
