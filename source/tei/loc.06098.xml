    <text type="letter">
        <body>
            <opener>
                <dateline>
                    <name type="place">Camden New Jersey<ptr target="loc.06098_n1"/></name>
                    <date when="1885-09-08">Sept. 8 noon</date><ptr target="loc.06098_n2"/>
