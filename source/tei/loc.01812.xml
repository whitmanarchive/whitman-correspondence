    <text type="letter">
        <body>

            <pb xml:id="leaf001r" facs="loc.01812.001.jpg" type="recto"/>
            <!--letterhead: Perrigo &amp; Finn-->
            <opener>
                <address><addrLine><handShift rend="print"/>Lockport, N.Y.</addrLine></address>
                <dateline>
                    <date when="1867-08-26"><handShift rend="manuscript"/>August 26, <handShift rend="print"/>18<handShift rend="manuscript"/>67&#8212;</date>
                </dateline>
                <address><addrLine>Washington D.C.</addrLine></address>
                <salute>Dear Sir:&#8212;</salute>
            </opener>

            <p>Send me two <unclear reason="illegible" cert="low" resp="#js">full</unclear> copies
                "C.O.D." of Your "Leaves of Grass"<!--annotation-->&#8212;best style &amp;
                oblige</p>


            <closer><salute>Yours Very Truly</salute>
                <signed><hi rend="underline">Geo. W.
                    Perrigo</hi><!-- <ptr target="n0472"/> --><!--unique underline with two hashmarks--></signed>
            </closer>
            <!--no verso image available on Feinberg <pb xml:id="leaf001v" facs="loc.01812.002.jpg" type="verso"/>-->
