    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="bpl.00002.001.jpg" type="recto"/>
            <opener>
                <dateline>
                    <handShift rend="printed"/>
                    <name type="person">Henry Romeike</name>
                    <name type="place">Branches: London, Paris, Berlin, Washington D.C.</name>
                    <name type="place">110 Fifth Avenue</name>
                    <name type="place">New York</name>
                    <date when="1891-09-10"><handShift rend="manuscript"/>Sept 10&#8212;<handShift rend="printed"/>189<handShift rend="manuscript"/>1</date> 
                </dateline>
                <salute>Mr Walt Whitman</salute>
                <salute>Dear Sir:&#8212;</salute>
            </opener>
            <p><handShift rend="printed"/>Your work is attracting considerable attention.
            Will you allow me to send you all notices concerning
            it that may appear in the papers of the United States
            and Canada.?<ptr target="bpl.00036_n2"/></p>
            <closer>
                <salute><handShift rend="printed"/>Yours faithfully,</salute>
                <signed><handShift rend="manuscript"/>Henry Romeike</signed>
            </closer>
            <pb xml:id="leaf001rv" facs="bpl.00002.002.jpg" type="verso"/>
        </body>
