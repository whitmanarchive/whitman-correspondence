    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc_vm.00862_large.jpg" type="recto"/>
            <opener>
                <dateline rend="right">
                    <handShift rend="printed"/>
                    <name type="place">J. R. DAYTON.</name>
                    <name type="place">R. W. WALDO.</name>
                    <name type="place">OFFICE OF</name>
                    <name type="place">DAYTON &amp; WALDO, COTTON BROKERS,</name>
                    <name type="place">No. 129 Pearl Street,</name>
                    <name type="place">New York <handShift rend="manuscript"/></name>
                        <date when="1873-01-15">Jan 15 <handShift rend="printed"/>18<handShift rend="manuscript"/>73<ptr target="loc.01974_n1"/></date>
                </dateline>
                <salute>Walt Whitman Esq—</salute>
                <salute>Dear Sir</salute>
            </opener>
            <p>Please send me 12 copies of "The good grey poet"<ptr target="n0272"/> &amp; mark them
                C.O.D. to the above address and oblige</p>
            <closer>
                <salute rend="right">Yours truly</salute>
                <signed rend="right"><hi rend="underline">R. W. Waldo</hi></signed>
            </closer>
            <pb xml:id="leaf001v" facs="loc_vm.00861_large.jpg" type="verso"/>
            <pb xml:id="leaf002r" facs="loc_vm.00864_large.jpg" type="recto"/>
            <note resp="#ww" type="editorial">RW Waldo</note>
            <pb xml:id="leaf002v" facs="loc_vm.00863_large.jpg" type="verso"/>
        </body>
    </text>
