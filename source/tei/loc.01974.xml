    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc.01974.001_large.jpg" type="recto"/>
            
            <opener>
                <handShift rend="printed"/>
                <address>
                    <addrLine>OFFICE OF</addrLine>
                    <addrLine>DAYTON &amp; WALDO, COTTON BROKERS,</addrLine>
                    <addrLine>No. 129 Pearl Street,</addrLine>
                </address>
                <dateline rend="right">
                    <name type="place">New York </name>
                    <handShift rend="manuscript"/>
                    <date when="1873-01-15"><handShift rend="printed"/>18<handShift rend="manuscript"/>73</date>
                </dateline>
                <salute>Walt Whitman Esq&#8212;</salute><salute>Dear Sir</salute>
            </opener>
                

            <p>Please send me 12 copies of "The good gray poet" &amp; mark them C.O.D. to the above address and oblige</p>
            
            <closer>
                <salute rend="right">Yours truly</salute>
                <signed rend="right"><hi rend="underline">R.W. Waldo</hi></signed>
            </closer>
            
          <pb xml:id="leaf001v" facs="loc.01974.002_large.jpg" type="verso"/>
