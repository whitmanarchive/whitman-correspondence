    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc.00719.001.jpg" type="recto"/>
            <note type="editorial" resp="#ww" place="top">green card sent Sept 22</note>
            <opener>
                <dateline>
                    <name type="place">Lynn Mass</name>
                    <date when="1885-09-16">9/16/85</date>
                </dateline>
                <salute rend="right">Walt Whitman Esq</salute>
                <salute>Dear Sir</salute>
            </opener>
            <p> Please inform me the price of "Leaves of Grass" delivered here</p>
            <closer>
                <salute>Yours truly</salute>
                <signed>A. B. Nourse</signed>
                <address><addrLine>8 Pinkham Street.</addrLine></address>
            </closer>
            <pb xml:id="leaf001v" facs="loc.00719.002.jpg" type="verso"/>
