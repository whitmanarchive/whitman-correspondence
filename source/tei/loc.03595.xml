    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc.03595.001_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <handShift rend="printed"/>
                    <name type="person">Henry Romeike</name>
                    <name type="place">Branches: London, Paris, Berlin, Washington D.C.</name>
                    <name type="place">110 Fifth Avenue.<ptr target="loc.03595_n1"/></name>
                    <date when="1891-08-01">New York, <handShift rend="manuscript"/>Aug. 1, <handShift rend="printed"/>189<handShift rend="manuscript"/>1.</date>
                </dateline>
                <salute>Mr. Whitman</salute>
                <salute>Dear Sir;&#8212;</salute>
            </opener>
            <p><handShift rend="printed"/>Your work is attracting considerable attention. Will you allow me to send 
                you all notices concerning it that may appear in the papers of the United States and Canada.?<ptr target="loc.03595_n4"/></p>
            <closer>
                <salute>Yours faithfully,</salute>
                <signed rend="right"><handShift rend="manuscript"/>Henry Romeike</signed>
            </closer>

