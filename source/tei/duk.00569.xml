    <text type="letter">
        
        <body>
            
            <pb xml:id="leaf001r" facs="duk.00569.001.jpg" type="recto"/>
            
            <opener>
                <salute>dear Walt<ptr target="duk.00569_n1"/></salute>
            </opener>
            
            <p>i will write
            to you to morrow all
            the particulars<ptr target="duk.00569_n2"/>
            i was
            doubly glad to get
            this letter<ptr target="duk.00569_n5"/>
                <choice><orig>dont</orig><reg>don't</reg></choice> send it to
                han<ptr target="duk.00438_n4"/> at present</p>
            <closer>
                <signed>your mother<ptr target="duk.00425_n3"/></signed>
            </closer>
            
            <pb facs="duk.00343.009.jpg" type="verso" xml:id="leaf001v"/>
        </body>
