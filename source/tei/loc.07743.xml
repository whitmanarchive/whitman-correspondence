    <text type="letter">
        <body>
            <opener>
                <dateline>
                    <name type="place">David McKay</name>
                    <name type="place">bookseller,</name>
                    <name type="place">9th st. opp: P O Phila:<ptr target="loc.07743_n1"/></name>
