    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc.02233.001_large.jpg" type="recto"/>
            <p>
                With<ptr target="loc.02233_n1"/> loyal affection and best wishes<ptr target="loc.02233_n2"/>
              to my dear poet. God bless him!
            </p>
                <pb xml:id="leaf001v" facs="loc.02233.002_large.jpg" type="verso"/>
                <lg type="linegroup">
                    <l>Splendor of ended day<ptr target="loc.02233_n3"/></l> 
                    <l>Be but the door</l> 
                    <l>Opening the endless way&#8212;</l>
                    <l>Life evermore!</l>               
            </lg>
            <closer>
                <date when="1892-02">February 1892.</date>
            </closer>
            <pb xml:id="leaf002r" facs="loc.02233.003_large.jpg" type="recto"/>
