    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc_vm.00860_large.jpg" type="recto"/>
            <opener>
                <salute>Walt Whitman Esq</salute>
                <salute>Dear Sir</salute>
            </opener>
            <p>I will Sell My Lot on Royden St—for $450—all clear of incumbrance.</p>
            <closer>
                <salute>Yours Respectfully</salute>
                <signed><hi rend="underline">Thos A Wilson</hi></signed>
                <dateline>
                    <date when="1874-05-26">May 26/74</date>
                </dateline>
            </closer>
            <pb xml:id="leaf001v" facs="loc_vm.00859_large.jpg" type="verso"/>
            <note type="editorial" resp="#ww" place="top">Thos A Wilson</note>
        </body>
    </text>
