    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc.03501.002_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <date when="1890-10-29">Oct: 29 '90</date>
                    <name type="place">Press newspaper counting room</name>
                </dateline>
            </opener>
            <p>
                If convenient send me by this young man<ptr target="n5001"/> ten (10) papers Wednesday Oct: 22—and
                4 papers of Sunday Oct: 26—fourteen altogether
            </p>
            <closer>
                <signed>Walt Whitman</signed>
                <address>
                    <addrLine>328 Mickle St: Camden</addrLine>
                </address>
            </closer>
