               <name type="place"><handShift rend="printed"/>Farringford,</name>
                <name type="place"><handShift rend="printed"/>Freshwater,</name>
                <name type="place"><handShift rend="printed"/>Isle of Wight.<ptr target="loc.04274_n1"/></name>
