    <text type="letter">
        <body>

          
            <pb xml:id="leaf001r" facs="loc.01968.001_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <name type="place">Arlington, Mass.</name>
                    <date when="1876-04-26">Apr. 26, 1876.</date>
                </dateline>
                <salute>Walt Whitman &#8212;</salute>
                <salute>My Dear Friend:</salute>
            </opener>

            <p>I enclose a P.O. order for $10, in payment for a copy of your last book "Two Rivulets"&#8212;if it is now ready, or whenever it is ready.</p>
            <closer>
                <salute>As always, sincerely your friend</salute>
                <signed><unclear reason="illegible" cert="high" resp="#js">Jn.T.Trowbridge</unclear></signed>
            </closer>
            
        </body>
    </text>
