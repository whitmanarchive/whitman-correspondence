    <text type="letter">
        <body>

            <!--no images available yet <pb xml:id="leaf001r" facs="nyp.00389.001.jpg" type="recto"/>-->
            <opener>
                <dateline><address><addrLine>Department of Justice,</addrLine><addrLine>Washington,</addrLine></address>
                    <date when="1873-03-10">Mar. 10, 1873.</date>
                </dateline>
                <salute>Walt Whitman, Esq.,</salute>
                <salute>Sir:</salute>
            </opener>

            <p>You are hereby transferred from the office of the Attorney General<!--annotation-->
                to a clerkship of the third class<!--annotation--> in the office of Solicitor of the
                Treasury<!-- <ptr target="n0574"/> -->, to take effect on the 1st instant.</p>


            <closer><salute>Very respectfully</salute>
                <signed>George H. Williams<!-- <ptr target="n0716"/> -->,</signed>
                <signed>Attorney General.</signed>
            </closer>

            <!--no images available yet <pb xml:id="leaf001v" facs="nyp.00389.002.jpg" type="verso"/>-->
        </body>
    </text>
