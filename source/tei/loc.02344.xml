            <opener> 
                <salute><handShift rend="typescript"/>To Mr.<handShift rend="manuscript"/>Whitman</salute>
                <salute><handShift rend="typescript"/>Dear Sir:—</salute>
            </opener>
            <p><handShift rend="typescript"/>The current number of the Weekly 
                ILLUSTRATED AMERICAN contains an article<ptr target="loc.02344_n3"/> of interest to you.
            </p>
            <closer>
                <salute><handShift rend="typescript"/>Yours Truly,</salute>
                <signed>THE ILLUSTRATED AMERICAN PUB. CO.</signed>
                <signed>(T.)</signed>
                <dateline>
                    <name type="place">New York,<ptr target="loc.02344_n1"/></name>
                    <date when="1891-11-23">Nov. 23" 1891.</date>
                </dateline>
            </closer>
            <pb xml:id="leaf001v" facs="loc.02344.002_large.jpg" type="verso"/> 
            <note type="editorial" place="top" resp="#ht"><handShift rend="manuscript"/>see notes Nov 24 1891</note>
