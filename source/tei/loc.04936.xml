    <text type="letter">
        <body>
            <opener>
                <pb xml:id="leaf001r" facs="loc_vm.01677_large.jpg" type="recto"/>
                <dateline>
                    <name type="place">431 Stevens Street</name>
                    <name type="place">Camden New Jersey</name>
                    <date when="1881-12-03" rend="right">Dec: 3, '81</date>
                </dateline>
                <salute>Dear Sir</salute>
            </opener>
            <p>I send you a fair proof of my Emerson article visit in hopes it will be convenient for you to print it in full.<ptr target="loc.04936_n1"/></p>
            <closer>
                <signed>Walt Whitman</signed>
