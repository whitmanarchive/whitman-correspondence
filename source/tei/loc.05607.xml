    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc_nk.00428_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <handShift rend="printed"/>
                    <title>THE WESTERN UNION TELEGRAPH COMPANY.</title>
                    <note type="authorial"><handShift rend="manuscript"/>15 paid</note>
                    <name type="place"><handShift rend="printed"/>Received at NO 7 NORTH 3D STREET.</name>
                    <date when="1882"><handShift rend="manuscript"/>4 <handShift rend="printed"/>1882.</date>
                    <name type="place">Dated <handShift rend="manuscript"/>Boston Mass</name>
                    <date when="1882-05-16">5/16</date>
                </dateline>
                <salute><handShift rend="printed"/>To <handShift rend="manuscript"/>Walt Whitman</salute>
                <address><addrLine>431 Stevens St</addrLine></address>
            </opener>
            <p>
                At what hour tomorrow Wednesday
                can I call on you 
                Reply to Gilsey House New York
            </p>
            <closer>
                <signed>B H Ticknor</signed>
            </closer>
            <pb xml:id="leaf001v" facs="loc_nk.00429_large.jpg" type="recto"/>
