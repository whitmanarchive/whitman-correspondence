    <text type="letter">
        <body> 

            <pb xml:id="leaf001r" facs="loc_ej.00929c.jpg" type="recto"/>

            <opener>
                <dateline rend="right">
                    <date when="1880-12-10">12.10.80.</date><ptr target="loc.05759_n1"/>
                </dateline>
                <salute>Mr. Walt Whitman:</salute>
                <salute>Respected Sir,</salute>
            </opener>

            <p>Your kindness was received and <unclear reason="illegible" resp="#nhg" cert="low">I</unclear> hopefully appreciated this evening.</p>

            <p>Please accept my sincere thanks for your favor.</p>

            <closer>
                <salute>Very gratefully yours</salute>
                <signed>Hannah L. Taylor</signed>
                <address><addrLine>Cambridge, N.J.</addrLine></address>
            </closer>
           </body> 
