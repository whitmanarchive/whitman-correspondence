    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc_vm.02489_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <handShift rend="typescript"/>
                    <name type="place">Jersey City,</name>
                    <date when="1891-08-19">Aug. 19th. 1891.</date>
                </dateline>
                <salute><handShift rend="typescript"/>Mr Walt. Whitman,</salute>        
                <salute>Camden, N.J.</salute>
                <salute>Dear Sir:—</salute>
            </opener>
            <p><handShift rend="typescript"/>Please send me by express, C.O.D. or as you may choose, a copy of your 
                Farewell.<ptr target="loc.07894_n2"/></p>
            <closer>
                <handShift rend="typescript"/>
                <salute>Very sincerely yours,</salute>
                <signed>B. A. Watson,</signed>
                <address><addrLine>108 York st.</addrLine></address>
            </closer>
            <postscript>
                <p><handShift rend="typescript"/>Dictated.</p>
            </postscript>
            <pb xml:id="leaf001v" facs="loc_vm.02490_large.jpg" type="verso"/>
