    <text type="letter">
        <body>

            <pb xml:id="leaf001r" facs="loc.01898.001.jpg" type="recto"/>

            <opener><handShift rend="printed"/>
                <dateline>
                    <address><addrLine>Crown Buildings.</addrLine><addrLine>London, 188, Fleet Street.</addrLine><addrLine><handShift rend="manuscript"/>Mr. J.S. Redfield<ptr target="n0499"/></addrLine><addrLine>in <choice><abbr>a/c</abbr><expan>account</expan></choice> with</addrLine><addrLine><handShift rend="printed"/>Sampson Son, Marston, Son &amp; Searle.</addrLine></address>
                    <date when="1872-12-31"><handShift rend="manuscript"/>Dec 31 1872</date>
                </dateline>
            </opener>

            <ab part="N">By <choice>
                    <abbr>Amt</abbr>
                    <expan>Amount</expan>
                </choice> as per your <choice>
                    <abbr>a/c</abbr>
                    <expan>account</expan>
                </choice> 112<choice>
                    <orig status="unremarkable"> </orig>
                    <reg>.</reg>
                </choice>50 less Goods on hand</ab>
            <p><table>
                <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">48 <hi rend="italic">Whitman's Vistas</hi></cell>
                    <cell role="data" rows="1" cols="1">.45</cell>
                    <cell role="data" rows="1" cols="1">21<choice>
                            <orig status="unremarkable"> </orig>
                            <reg>.</reg>
                        </choice>60</cell>
                    <cell role="data" rows="1" cols="1"/>
                </row>
                <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">41 <hi rend="italic">Leaves Grass</hi></cell>
                    <cell role="data" rows="1" cols="1"><hi rend="underline">1.80</hi></cell>
                    <cell role="data" rows="1" cols="1"><hi rend="underline">73<choice>
                                <orig status="unremarkable"> </orig>
                                <reg>.</reg>
                            </choice>80</hi></cell>
                    <cell role="data" rows="1" cols="1"><hi rend="underline">95<choice>
                                <orig status="unremarkable"> </orig>
                                <reg>.</reg>
                            </choice>40</hi></cell>
                </row>
                <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">$17.10</cell>
                </row>
            </table></p>
            <closer>
                <address><addrLine>To Walt Whitman Esq.</addrLine><addrLine>Solicitor's Office</addrLine><addrLine>Treasury</addrLine><addrLine>Washington</addrLine><addrLine>D.C.</addrLine></address>
