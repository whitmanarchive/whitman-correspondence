  <text type="letter">
    <body>
      <pb xml:id="leaf001r" facs="loc.01291.001_large.jpg" type="recto"/>
      <opener>
        <dateline>
          <name type="place" rend="center"><handShift rend="printed"/>Department of Justice</name>
          <name type="place" rend="right">Washington.</name>
          <date when="1871-05-19" rend="right"><handShift rend="manuscript"/>May 19, <handShift rend="printed"/>187<handShift rend="manuscript"/>1.</date>
        </dateline>
          <salute>Messrs. Church,</salute><ptr target="loc.01707_n1"/>
          <salute>My friends,</salute>
      </opener>

      <p>If convenient, please remit to me, here, a check for $25 in pay for "O Star of France".</p>
      
      <closer>
        <salute>Yours truly</salute>
        <signed rend="right">Walt Whitman</signed>
      </closer>
      <pb xml:id="leaf001v" facs="loc.01291.002_large.jpg" type="verso"/>
