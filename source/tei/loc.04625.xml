    <text type="letter">
        <body>
            <pb xml:id="leaf001r" facs="loc_sd.00391.jpg" type="recto"/>
            <opener>
                <dateline>
                    <name type="place">Albany</name>
                    <date when="1885-06-13">June 13/85</date>
                </dateline>
                <salute>Mr. Walt Whitman</salute>
                <salute>Dear Sir:</salute>
            </opener>
            <p>Will you kindly favor me with your Autograph on card enclosed and greatly oblige</p>
            <closer>
                <salute>Very Truly Yours</salute>
                <signed>Geo Weaver</signed>
            </closer>
            <pb xml:id="leaf001v" facs="loc_sd.00390.jpg" type="verso"/>
