    <text type="letter">
        <body>


            <pb xml:id="leaf001r" facs="loc.01609.001.jpg" type="recto"/>
            <opener>
                <dateline rend="right">
                    <address><addrLine>Senate <unclear reason="illegible" cert="medium" resp="#km">W</unclear></addrLine></address>
                    <date when="1867-01-04">Jan 4 67</date></dateline>
                <salute>Dear Sir:</salute>
            </opener>
            <p>Mr Hyde<!-- <ptr target="n0299"/> --> of Burlington, Vt, has sent to me a small package for you. It is at my room
                419 N.Y., <gap reason="unclear" extent="one word" unit="chars" quantity="2" resp="#br"/>. Please call for it.</p>
            <closer>
                <salute><unclear reason="illegible" cert="medium" resp="#km">Yours
                    truly</unclear></salute>
                <signed>George F. Edmunds<!-- <ptr target="n0207"/> --></signed>
                <address>
                    <addrLine>Mr W Whitman</addrLine>
                    <addrLine>Atty <choice><abbr>Gener</abbr><expan>General's</expan></choice> Office</addrLine>
                </address>
            </closer>


        </body>
