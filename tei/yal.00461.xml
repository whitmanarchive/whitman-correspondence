            <pb xml:id="leaf001r" facs="yal.00339.002b_large.jpg" type="recto"/>
            <opener>
                <dateline>
                    <name type="place"><gap reason="cut away"/><handShift rend="printed"/>ss.<ptr target="yal.00461_n1"/></name>
                    <date when="1880-03-20"><handShift rend="manuscript"/>March 20 <handShift rend="printed"/>188<handShift rend="manuscript"/>0</date>
                </dateline>
                <salute><gap reason="cut away"/>ar Mr Whitman</salute>
            </opener>
            <p>
                <gap reason="cut away"/> you at hand
                <gap reason="cut away"/> do me a
                <gap reason="cut away"/> poem for <unclear reason="illegible" resp="#nhg" cert="low">Mi</unclear>
                <gap reason="cut away"/><unclear reason="illegible" resp="#nhg" cert="low">undler</unclear> of the
                <gap reason="cut away"/> Press? If so,
                <gap reason="cut away"/> send it with
                <gap reason="cut away"/> to yours
            </p>
            <closer>
                <salute>Sincerely</salute>
                <signed><unclear reason="illegible" resp="#nhg" cert="medium">M. C.</unclear> Wheeler</signed>
                <signed><gap reason="illegible" unit="words" quantity="2"/></signed>
            </closer>
            <pb xml:id="leaf001v" facs="yal.00339.001_large.jpg" type="verso"/>
