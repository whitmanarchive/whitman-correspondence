    <text type="letter">
        
        <body>
                
                <pb xml:id="leaf001r" facs="tex.00349.001.jpg" type="recto"/>
            
                <opener> 
                    <salute>dear walter<ptr target="tex.00349_n2"/></salute>
                </opener>
                
                <p><choice><orig>dont</orig><reg>don't</reg></choice> be worried i shall be
                    better dont come till you can walk
                    good and without injury to your
                    getting fully recovered<ptr target="tex.00349_n4"/></p>
                
                <closer>
                    <salute>good bie my dear beloved walter</salute>
                    <signed>your mother<ptr target="duk.00425_n3"/></signed>
                </closer>
                
                <pb xml:id="leaf001v" type="verso"/>

        </body>
        
        
