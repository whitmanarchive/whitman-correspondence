    <text type="letter">
        <body>

            <pb xml:id="leaf001r" facs="loc_ej.00941.jpg" type="recto"/>

            <note type="editorial" place="top" resp="#ww">Books sent</note>

            <opener>
                <!-- letterhead includes image -->
                <dateline rend="right">
                    <name type="place"><handShift rend="printed"/>Commonwealth of
                        Pennsylvania<ptr target="loc.05777_n1"/> <lb/>State Library</name>
                        <name type="place">C. L. Ehrenfeld, <lb/>Librarian.</name>
                    <date when="1880-10-25">Harrisburg <handShift rend="manuscript"/>Oct. 25 
                            <handShift rend="printed"/>18<handShift rend="manuscript"/>80</date>
                </dateline>
                <salute>Walt Whitman Esq.</salute>
                <salute>Dear Sir:&#8212;</salute>
            </opener>

            <p>Please send me a copy of the Centennial edition of your works for this Library, with
                bill, and I will remit you the <choice><abbr>amt.</abbr><expan>amount</expan></choice> at once.</p>

            <closer>
                <salute>Yours truly,</salute>
                <signed>C.L. Ehrenfeld</signed>
            </closer>
            
            <pb xml:id="leaf001v" facs="loc_ej.00940.jpg" type="verso"/> 
            <!-- Whitman wrote manuscript poetry and prose on this page - unrelated to letter -->

